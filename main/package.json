{
	"name": "next-redux-cookie-wrapper",
	"version": "2.1.2",
	"description": "Sync a subset of your Redux state with cookies in Next.js ðŸª âœ¨",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=12"
	},
	"source": "src/index.ts",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"start": "tsdx watch",
		"test": "tsdx test --passWithNoTests --coverage",
		"test:watch": "npm run test -- --watch",
		"tsc": "tsc",
		"build": "tsdx build --tsconfig tsconfig.build.json",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"prepare": "npm run build",
		"release": "semantic-release",
		"prepack": "cp ../README.md ."
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bjoluc/next-redux-cookie-wrapper.git"
	},
	"author": "bjoluc <mail@bjoluc.de>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/bjoluc/next-redux-cookie-wrapper/issues"
	},
	"homepage": "https://github.com/bjoluc/next-redux-cookie-wrapper#readme",
	"dependencies": {
		"@types/cookie": "0.4.1",
		"fast-deep-equal": "^3.1.3",
		"immer": "^9.0.15",
		"lodash": "^4.17.21",
		"lodash-es": "^4.17.21",
		"lz-string": "^1.4.4",
		"nookies": "2.5.2",
		"type-fest": "^2.17.0"
	},
	"peerDependencies": {
		"next-redux-wrapper": ">=6"
	},
	"devDependencies": {
		"@size-limit/esbuild": "^7.0.8",
