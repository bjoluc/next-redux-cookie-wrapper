		"@types/lz-string": "^1.3.34",
		"@types/node": "^18.0.6",
		"@types/set-cookie-parser": "^2.4.2",
		"next": "^12.2.3",
		"next-redux-wrapper": "^7.0.5",
		"node-mocks-http": "^1.11.0",
		"redux": "^4.2.0",
		"set-cookie-parser": "^2.5.0",
		"size-limit": "^7.0.8",
		"tsdx": "^0.14.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"size-limit": [
		{
			"path": "dist/next-redux-cookie-wrapper.cjs.production.min.js",
			"limit": "15 KB"
		},
		{
