{
  "name": "next-redux-cookie-wrapper",
  "version": "1.1.4",
  "description": "Use redux-persist and redux-persist-cookie-storage in Next.js â€“ with ease",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "tsc": "tsc --noEmit",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoluc/next-redux-cookie-wrapper.git"
  },
  "author": "bjoluc <mail@bjoluc.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjoluc/next-redux-cookie-wrapper/issues"
  },
  "homepage": "https://github.com/bjoluc/next-redux-cookie-wrapper#readme",
  "dependencies": {
    "cookies": "0.8.0",
    "cookies-js": "1.2.3",
    "next-redux-wrapper": "5.0.0",
    "react": "^17.0.1",
    "redux-persist": "6.0.0",
    "redux-persist-cookie-storage": "1.0.0"
  },
  "peerDependencies": {
    "next": "9.x",
    "react": "17.x",
    "redux": "4.x"
  },
  "devDependencies": {
    "@bjoluc/semantic-release-config-npm": "^1.0.0",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "@types/cookies": "^0.7.6",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@types/react": "17.0.0",
    "@types/react-test-renderer": "17.0.0",
    "commitizen": "^4.2.3",
    "conventional-changelog-conventionalcommits": "^4.5.0",
    "cz-conventional-changelog": "3.3.0",
    "delay": "4.4.0",
    "husky": "4.3.8",
    "import-sort-style-module": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "next": "^10.0.5",
    "node-mocks-http": "1.10.1",
    "prettier": "2.2.1",
    "prettier-plugin-import-sort": "0.0.6",
    "react-test-renderer": "17.0.1",
    "redux": "4.0.4",
    "rollup": "2.38.0",
    "rollup-plugin-typescript2": "0.29.0",
    "semantic-release": "^17.3.8",
    "ts-jest": "26.4.4",
    "typescript": "3.9.7"
  },
  "browser": {
    "cookies": false
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 100
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ],
      "scope-case": [
        1,
        "never",
        [
          "lower-case"
        ]
      ]
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts(x)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@bjoluc/semantic-release-config-npm"
  }
}
