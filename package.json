{
  "name": "next-redux-cookie-wrapper",
  "version": "1.1.0",
  "description": "Use redux-persist and redux-persist-cookie-storage in Next.js â€“ with ease",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "source": "src/index.tsx",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "tsc": "tsc --noEmit",
    "build": "rollup -c",
    "watch": "rollup -cw",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bjoluc/next-redux-cookie-wrapper.git"
  },
  "author": "bjoluc <mail@bjoluc.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjoluc/next-redux-cookie-wrapper/issues"
  },
  "homepage": "https://github.com/bjoluc/next-redux-cookie-wrapper#readme",
  "dependencies": {
    "cookies": "0.8.0",
    "cookies-js": "1.2.3",
    "next-redux-wrapper": "5.0.0",
    "react": "16.13.1",
    "redux-persist": "6.0.0",
    "redux-persist-cookie-storage": "1.0.0"
  },
  "peerDependencies": {
    "next": "9.x",
    "redux": "4.x"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.5",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "25.2.1",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-test-renderer": "16.9.2",
    "commitizen": "4.1.2",
    "conventional-changelog-conventionalcommits": "4.3.0",
    "cz-conventional-changelog": "3.2.0",
    "delay": "4.3.0",
    "husky": "4.2.5",
    "jest": "25.5.4",
    "lint-staged": "10.2.2",
    "next": "9.3.2",
    "node-mocks-http": "1.8.1",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "redux": "4.0.4",
    "rollup": "2.10.0",
    "rollup-plugin-typescript2": "0.27.1",
    "semantic-release": "17.0.7",
    "ts-jest": "25.5.1",
    "typesafe-actions": "5.1.0",
    "typescript": "3.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        2,
        "always",
        [
          "sentence-case"
        ]
      ],
      "scope-case": [
        1,
        "never",
        [
          "lower-case"
        ]
      ]
    }
  },
  "lint-staged": {
    "{src,tests}/**/*.ts(x)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
